    "@tanstack/react-router-devtools": "^1.121.2",
    "@tanstack/router-plugin": "^1.121.2",
    "axios": "^1.10.0",
    "lucide-react": "^0.525.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-force-graph": "^1.48.0",
    "react-force-graph-2d": "^1.28.0",
    "react-hot-toast": "^2.5.2",
